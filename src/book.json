{
    "title": "Workshop GeoExt3",
    "description": "Learn how to use GeoExt 3 in your ExtJS applications…",
    "language": "en",
    "plugins": [
        "edit-link",
        "expandable-chapters",
        "image-captions",
        "richquotes"
    ],
    "variables": {
        "exerciseUrl": "http://localhost:4000/exercises"
    },
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/geoext/geoext3-ws/edit/master/src",
            "label": "Edit…"
        },
        "richquotes": {
            "task": {
                "alert": "info",
                "picto": "fa-file-code-o"
            },
            "exercise": {
                "alert": "info",
                "picto": "fa-file-code-o"
            }
        }
    }
}
